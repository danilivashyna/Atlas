{
  "title": "AURIS Phase B SLO Dashboard",
  "tags": ["AURIS", "Phase B", "HSI", "Stability", "Hysteresis"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROM",
        "label": "Prometheus",
        "query": "prometheus",
        "refresh": 1,
        "hide": 0
      },
      {
        "type": "textbox",
        "name": "window",
        "label": "window_id",
        "query": "global"
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Stability Score & EMA",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "atlas_stability_score{window_id=\"$window\"}",
          "legendFormat": "score (current)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        },
        {
          "expr": "atlas_stability_score_ema{window_id=\"$window\"}",
          "legendFormat": "ema (smoothed)",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Score",
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "green",
                "value": 0.8
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["lastNotNull", "mean"]
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "FAB Modes: Recommended vs Effective (Hysteresis)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "atlas_recommended_fab_mode{window_id=\"$window\"}",
          "legendFormat": "recommended (from Stability)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        },
        {
          "expr": "atlas_hysteresis_effective_mode{window_id=\"$window\"}",
          "legendFormat": "effective (after Hysteresis)",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        },
        {
          "expr": "atlas_hysteresis_desired_mode{window_id=\"$window\"}",
          "legendFormat": "desired (input to Hysteresis)",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Mode",
            "drawStyle": "line",
            "lineInterpolation": "stepBefore",
            "lineWidth": 2
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "GENTLE",
                  "color": "green"
                },
                "1": {
                  "text": "BALANCED",
                  "color": "yellow"
                },
                "2": {
                  "text": "ASSERTIVE",
                  "color": "orange"
                }
              }
            }
          ]
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Hysteresis: Switch Rate & Oscillation Rate",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "atlas_hysteresis_switch_rate_per_sec{window_id=\"$window\"}",
          "legendFormat": "switch_rate (switches/sec)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        },
        {
          "expr": "atlas_hysteresis_oscillation_rate{window_id=\"$window\"}",
          "legendFormat": "oscillation_rate (osc/window)",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Rate",
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "red",
                "value": 1.0
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["lastNotNull", "max", "mean"]
        }
      }
    },
    {
      "type": "gauge",
      "title": "Hysteresis: Dwell Ticks",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "atlas_hysteresis_dwell_ticks{window_id=\"$window\"}",
          "legendFormat": "dwell_ticks",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 25
              },
              {
                "color": "green",
                "value": 50
              }
            ]
          },
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "type": "gauge",
      "title": "Hysteresis: Age Ticks",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 8
      },
      "targets": [
        {
          "expr": "atlas_hysteresis_age_ticks{window_id=\"$window\"}",
          "legendFormat": "age_ticks",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 500
              },
              {
                "color": "red",
                "value": 900
              }
            ]
          },
          "min": 0,
          "max": 1000
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "type": "stat",
      "title": "Stability: Degradation Events",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "atlas_stability_degradation_events{window_id=\"$window\"}",
          "legendFormat": "total_events",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "colorMode": "background"
      }
    },
    {
      "type": "stat",
      "title": "Stability: Stable Ticks",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 16
      },
      "targets": [
        {
          "expr": "atlas_stability_stable_ticks{window_id=\"$window\"}",
          "legendFormat": "stable_ticks",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 100
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "colorMode": "background"
      }
    },
    {
      "type": "stat",
      "title": "Degradation Count (Total)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "atlas_stability_degradation_count{window_id=\"$window\"}",
          "legendFormat": "total_count",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROM}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 20
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "colorMode": "background"
      }
    },
    {
      "id": 901,
      "type": "timeseries",
      "title": "SELF Coherence (5m avg)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 38 },
      "targets": [
        { "expr": "self:coherence:5m_avg", "legendFormat": "coherence", "refId": "A" }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 902,
      "type": "timeseries",
      "title": "SELF Continuity (5m avg)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 38 },
      "targets": [
        { "expr": "self:continuity:5m_avg", "legendFormat": "continuity", "refId": "A" }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 903,
      "type": "timeseries",
      "title": "SELF Stress (5m avg, lower is better)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 46 },
      "targets": [
        { "expr": "self:stress:5m_avg", "legendFormat": "stress", "refId": "A" }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 0.3 }
            ]
          }
        }
      }
    }
  ],
  "annotations": {
    "list": [
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROM}"
        },
        "enable": true,
        "expr": "changes(atlas_stability_degradation_events{window_id=\"$window\"}[1m]) > 0",
        "name": "Degradation Events",
        "step": "1m",
        "tagKeys": "window_id",
        "titleFormat": "Degradation Detected",
        "textFormat": "Stability degraded"
      }
    ]
  }
}
